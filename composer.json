{
    "name": "yiisoft/mailer",
    "type": "library",
    "description": "Yii Mailer",
    "keywords": [
        "yii",
        "mailer"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/mailer",
        "issues": "https://github.com/yiisoft/mailer/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "require": {
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.1",
        "yiisoft/view": "^3.0@dev",
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "infection/infection": "^0.18",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "suggest": {
        "yiisoft/event-dispatcher": "Yii event dispatcher",
        "yiisoft/log": "Yii logging library"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Mailer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Mailer\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
