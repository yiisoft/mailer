{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php:\/\/stderr",
        "stryker": {
            "report": "master"
        }
    },
    "mutators": {
        "@default": true,
        "DecrementInteger": {
            "ignoreSourceCodeByRegex": [
                ".*mkdir\\(\\$filepath, 0777, true\\).*"
            ]
        }
    },
    "minMsi": 100
}
